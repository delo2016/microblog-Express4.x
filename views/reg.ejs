<div class="container container_bg" >
	<div class="row">
		<div class="col-sm-3"></div>
		<div class="col-sm-6 col-xs-12 my_edit" >
			<div class="row" id="edit_form" >
				<span class="pull-left" style="margin:15px;"></span>
				<span class="tips pull-right" style="margin:15px;"></span>
        <form class="form-horizontal" method="post">
          <legend>用户注册</legend>
          <div class="form-group">
            <label class="col-sm-2 control-label" for="username">用户名</label>
            <div class="col-sm-6">
              <input type="text" class="form-control" id="username" name="username">
            </div>
          </div>
          <div class="form-group" style="padding-top:10px;">
            <label class="col-sm-2 control-label" for="password">密码</label>
            <div class="col-sm-6">
              <input type="password" class="form-control" id="password" name="password">
            </div>
          </div>
          <div class="form-group" style="padding-top:10px;">
            <label class="col-sm-2 control-label" for="password-repeat">重复密码</label>
            <div class="col-sm-6">
              <input type="password" class="form-control" id="password-repeat" name="password-repeat">
            </div>
          </div>
          <div class="form-group" style="padding-top:10px;">
            <label class="col-sm-2 control-label"></label>
            <div class="col-sm-2">
              <button type="submit" class="btn btn-primary form-control">注册</button>
            </div>
          </div>
        </form>
			</div>
      <div class="row"> <% include err.ejs %></div>
		</div>
	</div>
</div>

<div style="text-align:center;margin:50px 0; font:normal 14px/24px 'MicroSoft YaHei';">

<% include suffix.ejs %>
